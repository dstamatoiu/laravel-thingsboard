{
    "name": "jalallinux/laravel-thingsboard",
    "description": "ThingsBoard laravel client",
    "keywords": [
        "JalalLinuX",
        "thingsboard"
    ],
    "homepage": "https://github.com/jalallinux/laravel-thingsboard",
    "license": "MIT",
    "authors": [
        {
            "name": "JalalLinuX",
            "email": "smjjalalzadeh93@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0|^8.1|^8.2",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^7.0",
        "illuminate/container": "^9.0|^10.0",
        "illuminate/contracts": "^9.0|^10.0",
        "illuminate/http": "^9.0|^10.0",
        "illuminate/support": "^9.0|^10.0",
        "jenssegers/model": "^1.5",
        "spatie/laravel-enum": "^3.0",
        "vkovic/laravel-custom-casts": "^1.4"
    },
    "require-dev": {
        "orchestra/testbench": "^7.0|^8.0",
        "phpunit/phpunit": "^9.4"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "JalalLinuX\\Thingsboard\\": "src"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "JalalLinuX\\Thingsboard\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "format": "php-cs-fixer fix --allow-risky=yes",
        "analyse": "phpstan analyse"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "JalalLinuX\\Thingsboard\\LaravelThingsboardServiceProvider"
            ]
        }
    }
}
